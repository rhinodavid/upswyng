{
  "name": "@upswyng/upswyng-server",
  "main": "src/index.ts",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "build": "yarn workspace @upswyng/upswyng-core build && sapper build --legacy",
    "cy:open": "yarn workspace @upswyng/upswyng-core build && cypress open",
    "cy:run": "yarn workspace @upswyng/upswyng-core build && cypress run",
    "data-setup-categories": "yarn workspace @upswyng/upswyng-core build && rollup --config data-pipeline.rollup.config.js && node __build__/setupCategories.js",
    "data-sync-strappd": "yarn workspace @upswyng/upswyng-core build && rollup --config data-pipeline.rollup.config.js && node __build__/syncStrappd.js",
    "data-update-algolia": "yarn workspace @upswyng/upswyng-core build && rollup --config data-pipeline.rollup.config.js && node __build__/updateAlgolia.js",
    "dev": "node showBuildNotification.js && sapper dev",
    "start": "node __sapper__/build",
    "test": "yarn workspace @upswyng/upswyng-core build && run-p --race dev unit",
    "unit": "yarn workspace @upswyng/upswyng-core build && jest --passWithNoTests"
  },
  "dependencies": {
    "@slack/web-api": "^5.6.0",
    "@upswyng/upswyng-core": "0.1.0",
    "@upswyng/upswyng-types": "0.1.0",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "bson": "^4.0.2",
    "bulma-ribbon": "^0.1.1",
    "bulma-switch": "^2.0.0",
    "bulma-tagsinput": "^2.0.0",
    "bulma-timeline": "^3.0.4",
    "canonical-instance": "^2.0.0",
    "compression": "^1.7.1",
    "connect-mongo": "^3.0.0",
    "cors": "^2.8.5",
    "debounce": "^1.2.0",
    "devalue": "^2.0.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "fast-deep-equal": "^2.0.1",
    "grant": "^4.6.2",
    "grant-express": "^4.6.2",
    "grant-oidc": "0.0.3",
    "javascript-time-ago": "^2.0.4",
    "json-string-formatter": "^1.0.1",
    "moment": "^2.24.0",
    "mongoose": "^5.6.13",
    "nano-equal": "^2.0.2",
    "num-words": "^1.0.1",
    "polka": "next",
    "rrule": "^2.6.2",
    "sirv": "^0.4.0",
    "svelte3-autocomplete": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@pyoner/svelte-ts-preprocess": "^1.2.1",
    "@rollup/plugin-json": "^4.0.1",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "@rollup/plugin-replace": "^2.3.0",
    "@rollup/plugin-typescript": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "algoliasearch": "^3.35.1",
    "autoprefixer": "^9.6.4",
    "bulma": "^0.7.5",
    "cli-progress": "^3.1.0",
    "cypress": "^3.4.1",
    "dotenv": "^8.1.0",
    "jest": "^24.9.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.18",
    "rollup": "^1.12.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript2": "^0.25.3",
    "sapper": "^0.27.9",
    "svelte": "^3.0.0",
    "svelte-forms": "^1.2.0",
    "svelte-preprocess": "^3.1.2",
    "svelte-scrollto": "^0.1.1",
    "svelte-select": "^3.1.2",
    "ts-jest": "^24.2.0",
    "tslib": "^1.10.0",
    "typescript": "^3.7.3"
  },
  "engines": {
    "node": "12.14.x"
  }
}
